{
  "env": {
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "globals": {
    "fetch": false
    // "Atomics": "readonly",
    // "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "import",
    "prettier"
  ],
  "rules": {
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off",
    "quotes": [
      "error",
      "double"
    ],
    "no-underscore-dangle": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"
  }
}
